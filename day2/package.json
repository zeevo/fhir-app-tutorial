{
  "name": "fhir-app-starter",
  "version": "2.0.1",
  "description": "Starter project for Next-gen FHIR Apps",
  "keywords": [
    "FHIR",
    "React",
    "Redux",
    "Redux-Saga",
    "Webpack",
    "ES6"
  ],
  "author": "Asymmetrik Ltd.",
  "contributors": [
    "Shane O'Neill <soneill@asymmetrik.com>"
  ],
  "bugs": "https://github.com/asymmetrik/fhir-app-starter/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asymmetrik/fhir-app-starter"
  },
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "fhirclient": "^2.3.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "yarn build:launch && react-scripts start",
    "build": "yarn build:launch && react-scripts build",
    "build:launch": "node ./internals/build.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint  --ext .js --ext .jsx src",
    "format": "prettier \"**/*.{js,jsx}\" --write",
    "changelog": "conventional-changelog -p angular -s -i CHANGELOG.md"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "handlebars": "^4.7.6",
    "prettier": "^2.0.5"
  }
}
